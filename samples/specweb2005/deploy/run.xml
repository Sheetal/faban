<?xml version="1.0" encoding="UTF-8"?>
<specweb2005Benchmark>
    <!-- jvmConfig is only needed for the cmdAgent since this benchmark is
         not java -->
    <jvmConfig>
        <javaHome>/usr/java</javaHome>
        <javaOptions> -server -Xmx840m -Xms840m </javaOptions>
    </jvmConfig>
    <runConfig>
        <description>Sample SPECWEB2005 benchmark run</description>
        <hostConfig>
            <host></host><!-- derived from the clients field -->
            <enabled>true</enabled>
            <cpus>0</cpus>
            <tools>NONE</tools>
            <userCommands></userCommands>
            <nodeid>1</nodeid>
            <port>2000</port>
        </hostConfig>
        <clients>starlost starlost:1200</clients>
        <clientDir>/export/home/w2005/sw2005-v1.02f.jar</clientDir>
        <scale>50</scale>
        <runControl unit="time">
            <rampUp>60</rampUp>
            <steadyState>120</steadyState>
            <rampDown>60</rampDown>            
        </runControl>
        <testtype>SPECweb_Support</testtype>
        <warmupSeconds>300</warmupSeconds>
        <threadRampupSeconds>300</threadRampupSeconds>
        <!-- threadRampdownSeconds>300</threadRampdownSeconds == rampDown>
        <runSeconds>300</runSeconds == steadyState -->
        <rampupSeconds>300</rampupSeconds>
        <rampdownSeconds>300</rampdownSeconds>
        <iterations>1</iterations>
        <parConns>1</parConns>
        <sleepTestIterations>1</sleepTestIterations>
        <olRun>1</olRun>        
    </runConfig>
    <webserverConfig>
        <hostConfig>
            <host>watergap</host>
            <enabled>true</enabled>
            <cpus>0</cpus>
            <tools>NONE</tools>
            <userCommands></userCommands>
        </hostConfig>
        <wsConfig>
            <wsPort>80</wsPort>
            <wsSSLport>443</wsSSLport>
        </wsConfig>
    </webserverConfig>
    <besimserverConfig>
        <hostConfig>
            <host>lindamar</host>
            <enabled>true</enabled>
            <cpus>0</cpus>
            <tools>NONE</tools>
            <userCommands></userCommands>
        </hostConfig>
        <besimConfig>
            <port>8000</port>
            <initScript>/BeSim/specweb2005_besim</initScript>
            <persistent>1</persistent>
        </besimConfig>
    </besimserverConfig>
    <supportParameters>
         <supportWorkloadDetail>
             <dynScript>spec05support.jsp</dynScript>
             <initScript>init.jsp</initScript>
             <imgPath>/suppdata/images</imgPath>
             <dynScriptPath>/suppjsp/</dynScriptPath>
             <downloadPath>/suppdata/downloads</downloadPath>
             <paddingDir>dynamic_padding/</paddingDir>
             <downloadSubdirs>500</downloadSubdirs>
             <baseSubdir>/subdir0000</baseSubdir>
             <firstSubdirNum>1</firstSubdirNum>
             <dynScript0>index.jsp</dynScript0>
             <dynScript1>search.jsp</dynScript1>
             <dynScript2>catalog.jsp</dynScript2>
             <dynScript3>product.jsp</dynScript3>
             <dynScript4>fileCatalog.jsp</dynScript4>
             <dynScript5>file.jsp</dynScript5>
             <dynScript6>download.jsp</dynScript6>
             <bufSize>20480</bufSize>
         </supportWorkloadDetail>
    </supportParameters>
    <bankingParameters>
         <bankingWorkloadDetail>
             <dynScript>spec05bank.jsp</dynScript>
             <initScript>init.jsp</initScript>
             <imgPath>/bankdata/images</imgPath>
             <dynScriptPath>/bankjsp/</dynScriptPath>
             <paddingDir>dynamic_padding/</paddingDir>
             <checkImageDir>images</checkImageDir>
             <checkImageSubdirs>500</checkImageSubdirs>
             <baseSubdir>/subdir0000</baseSubdir>
             <firstSubdirNum>1</firstSubdirNum>
             <userNumLen>10</userNumLen>
             <checkNumLen>6</checkNumLen>
             <checkImageRelPath>bankdata/images</checkImageRelPath>
             <dynScript0>login.jsp</dynScript0>
             <dynScript1>account_summary.jsp</dynScript1>
             <dynScript2>check_detail_html.jsp</dynScript2>
             <dynScript3>bill_pay.jsp</dynScript3>
             <dynScript4>add_payee.jsp</dynScript4>
             <dynScript5>post_payee.jsp</dynScript5>
             <dynScript6>quick_pay.jsp</dynScript6>
             <dynScript7>bill_pay_status_output.jsp</dynScript7>
             <dynScript8>profile.jsp</dynScript8>
             <dynScript9>change_profile.jsp</dynScript9>
             <dynScript10>order_check.jsp</dynScript10>
             <dynScript11>place_check_order.jsp</dynScript11>
             <dynScript12>transfer.jsp</dynScript12>
             <dynScript13>post_transfer.jsp</dynScript13>
             <dynScript14>logout.jsp</dynScript14>
             <dynScript15>check_detail_image.jsp</dynScript15>
             <dynScript16>check_detail_image.jsp</dynScript16>
             <serverCertFile>server.cer</serverCertFile>
             <serverCertPwd>specweb</serverCertPwd>
             <bufSize>20480</bufSize>
             <useSSL>1</useSSL>
         </bankingWorkloadDetail>
    </bankingParameters>
    <ecommerceParameters>
         <ecommerceWorkloadDetail>
             <dynScript>spec05ecommerce.jsp</dynScript>
             <initScript>init.jsp</initScript>
             <imgPath>/ecommerce/images</imgPath>
             <dynScriptPath>/ecomjsp/</dynScriptPath>
             <paddingDir>dynamic_padding/</paddingDir>
             <productlineNumLen>10</productlineNumLen>
             <productImageRelPath>ecommerce/images</productImageRelPath>
             <dirScaling>5</dirScaling>
             <productsPerDir>10</productsPerDir>
             <productImageSubdirs>10</productImageSubdirs>
             <baseSubdir>/subdir0000</baseSubdir>
             <firstSubdirNum>1</firstSubdirNum>
             <dynScript0>index.jsp</dynScript0>
             <dynScript1>search.jsp</dynScript1>
             <dynScript2>browse.jsp</dynScript2>
             <dynScript3>browse_productline.jsp</dynScript3>
             <dynScript4>productdetail.jsp</dynScript4>
             <dynScript5>customize.jsp</dynScript5>
             <dynScript6>customize.jsp</dynScript6>
             <dynScript7>customize.jsp</dynScript7>
             <dynScript8>cart.jsp</dynScript8>
             <dynScript9>login.jsp</dynScript9>
             <dynScript10>shipping.jsp</dynScript10>
             <dynScript11>billing.jsp</dynScript11>
             <dynScript12>confirm.jsp</dynScript12>
             <dynScript13>logout.jsp</dynScript13>
             <serverCertFile>server.cer</serverCertFile>
             <serverCertPwd>specweb</serverCertPwd>
             <bufSize>22528</bufSize>
             <useSSL>1</useSSL>
         </ecommerceWorkloadDetail>
    </ecommerceParameters>
</specweb2005Benchmark>
